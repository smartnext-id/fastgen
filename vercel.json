{
  "version": 2,
  "builds": [
    { "src": "web/index.html", "use": "@vercel/static" },
    { "src": "web/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "web/$1" }
  ]
}

{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.js" }
  ]
}
